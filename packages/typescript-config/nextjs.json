{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "jsx": "preserve",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "inlineSourceMap": true,
    "noEmit": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "target": "ES2017"
  }
}