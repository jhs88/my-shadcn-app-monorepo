{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "env": [
        "JAVA_API_HOST",
        "NEXT_PUBLIC_API_HOST",
        "SUPABASE_URL",
        "SUPABASE_ANON_KEY",
        "PORT"
      ],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [
        "dist/**",
        "build/**",
        ".next/**",
        "!.next/cache/**",
        "public/dist/**",
        "target/**"
      ]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": []
    },
    "test": {
      "dependsOn": [],
      "outputs": ["coverage/**"]
    }
  }
}
